<!ELEMENT lnci:cite  ( ( lnci:case | lnci:stat | lnci:juryinstr | lnci:reg | lnci:courtrule | lnci:sesslaw | lnci:patent | lnci:tmark | lnci:copyright | lnci:lawrev | lnci:periodical | lnci:book | lnci:oag | lnci:agencydecision | lnci:constitution | lnci:annot | lnci:execdoc | lnci:admininterp | lnci:misc | lnci:form )?,   lnci:metadata?, lnci:content? ) >

<!ATTLIST lnci:cite 


	  ID ID #IMPLIED 


	  type CDATA #IMPLIED 


	  srcstatus ( normal | temp | proposed | unpublished | incomplete ) "normal" 


	  impcit (main | subordinate) "main" 


	  anaphref NMTOKEN #IMPLIED 


	  judgelangref IDREF #IMPLIED 


	  pub ( advance | clp | supp | cum-supp | none ) 'none'


	  pub-status ( exclusive | pending | not-pub | none ) 'none'


	  clp-pub CDATA #IMPLIED


	  cdbii.locn CDATA #IMPLIED 


	  ri.key CDATA #IMPLIED


	  loadable NMTOKEN '1'


	  historycite NMTOKEN '0'


	  validate NMTOKEN '1'


	  citingmaterial (CASE | LEG | COMM | JRNL | DICT | DIGEST) #IMPLIED


	  normprotocol (lexstat | lexsee) #IMPLIED


	  citeref CDATA #IMPLIED 


	  normcite CDATA #IMPLIED 


	  cdbii.locnseqcd CDATA #IMPLIED


	  csidxval CDATA #IMPLIED 


	  xlink.href CDATA #IMPLIED 


	  xlink.role CDATA #IMPLIED 


	  xlink.title CDATA #IMPLIED 


	  xlink.show (new | parsed | replace) "replace" 


	  xlink.actuate (user | auto) "user" 


	  materialpriority NMTOKEN '2'


	  spanref IDREF #IMPLIED 


	  status (unval | valid | invalid | unresolved) "valid" 


	  statuscode CDATA #IMPLIED 


	  generated NMTOKEN '0' 


	  attreq NMTOKEN '0' 


	  lni CDATA #IMPLIED


	  lnci:ldi CDATA #IMPLIED 


	 >
